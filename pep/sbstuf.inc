     5 SUMRG(3),DADXR(3,3),DADVR(3,3),SUMRP(3,11),XMSB,
     6 NGA1,NGA2,NGM,NGN,NGK,NGALPH,NGR0,
     7 KP1,ICNTRL(i_mxeqn-1),KCNT,KTRG(i_mxtrg),LCNT,LTRG(i_mxtrg),
     8 NCZONP,NTZONP(i_mxtrg),NCTESP,NTTESP(i_mxtrg),KOUNT,
     9 JPOEV,MASSFL(30),PNTFLG,NPSTEP,MPSTEP,NPLTG(30),
     A DFLGC,DFLGH,DFLGS,DFLGP,DFLGA,DFLGT,DFLGDM(2),GGFGS(2)
      REAL*10 X0,DX0,
     1 MASSP,GOOSE,ALPH4,
     2 LTF11,LTF21,LTF31,LTF12,LTF22,LTF32,ALPH16,MASSP1,GAMT30,
     3 DADP,PHAR2,HSITB,DADXC,DADXH,DADXS,DADXP,DADXA,DADXT,
     4 DLTSUN,DLTBOD,SBFPOE,SUMR,SUMRB,SUMRG,
     5 DADXR,DADVR,SUMRP,XMSB,NGA1,NGA2,NGM,NGN,NGK,NGALPH,NGR0
      INTEGER*4 KP1,ICNTRL,KCNT,KTRG,LCNT,LTRG,NCZONP,NTZONP,
     1 NCTESP,NTTESP,KOUNT,JPOEV,NPSTEP,MPSTEP
      INTEGER*2 MASSFL,NPLTG
      LOGICAL*4 PNTFLG
      LOGICAL*1 DFLGC,DFLGH,DFLGS,DFLGP,DFLGA,DFLGT,DFLGDM
