C           EPOCHS COMPUTED DURING LIGHT-TIME ITERATION
      INTEGER*4 JDE(3)
      REAL*10 FR(3)
      EQUIVALENCE (JDE(1),JD),(FR(1),DSTF(1))
C        JDE(I)= JULIAN DAY NUMBER FOR BODY I INTEROLATION (I=1,2,3)
C        FR(I) = FRACTION OF DAY FOR BODY I INTERPOLATION (I=1,2,3)
C                     I=1 BODY IS EARTH-MOON BARYCENTER
C                     I=2 BODY IS MOON
C                     I=3 BODY IS PLANET
